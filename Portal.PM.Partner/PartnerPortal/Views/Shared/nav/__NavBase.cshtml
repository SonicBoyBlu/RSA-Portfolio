<a href="#" class="sidebar-toggle">
    <i class="fa fa-chevron-left"></i>
</a>

<a href="/" class="nav-logo-main">
    <img src="~/Content/images/Acme_logo.png" class="img-fluid" alt="Acme House Co." />
</a>

@if (GlobalSettings.ServerInfo.IsSandbox)
{
    <div class="d-none">
        <text>SPA: @GlobalSettings.ServerInfo.IsSpa</text>
    </div>
}
@if (Identity.Current.UserType == DataTypes.UserType.Owner)
{
    <h2 class="portal-label">Owner Portal</h2>
    Html.RenderPartial("~/Views/Shared/nav/NavOwner.cshtml");

}
else if (Identity.Current.UserType == DataTypes.UserType.Employee)
{
    <h2 class="portal-label">Team Portal</h2>
    Html.RenderPartial("~/Views/Shared/nav/NavEmployee.cshtml");
}

<script type="text/javascript">
    $(function () {
        $(".dropdown.active .fa-chevron-circle-down").addClass("open");
    });
</script>
